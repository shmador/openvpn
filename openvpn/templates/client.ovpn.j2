client
dev tun
proto udp
remote {{ server_ip }} 1194
resolv-retry infinite
nobind

persist-key
persist-tun

ca ca.crt
cert client1.crt
key client1.key
tls-auth ta.key 1

<ca>
{{ ca_cert_content }}
</ca>
<cert>
{{ client_cert_content }}
</cert>
<key>
{{ client_key_content }}
</key>
<tls-auth>
{{ ta_key_content }}
</tls-auth>

cipher AES-256-CBC
verb 3
